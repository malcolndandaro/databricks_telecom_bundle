{
    "datasets": [
        {
            "name": "a89e9b23",
            "displayName": "analise_performance_produto",
            "query": "SELECT a.* FROM databricks_telecom_bundle.billing_gold.analise_performance_produto a\n"
        },
        {
            "name": "a7b8d8bc",
            "displayName": "faturamento_por_categoria_e_mes",
            "query": "SELECT \n    d.ANO,\n    d.NM_MES,\n    to_date(d.MES_ANO, 'MM-yyyy') as mes_ano,\n    case when c.CLIENT = 'P' then 'Purpura'\n          when c.CLIENT = 'S' then 'Silver'\n          when c.CLIENT = 'G' then 'Gold'\n          when c.CLIENT = 'PL' then 'Platinum'\n          when C.CLIENT = 'V' then 'VIP'\n          end categoria_cliente,\n    SUM(f.VL_FTRA) AS FATURAMENTO_TOTAL,\n    SUM(f.VL_FTRA) * 1.05 as TARGET,\n    d.TRIMESTRE\nFROM databricks_telecom_bundle.billing_gold.fact_invoicing f\nJOIN databricks_telecom_bundle.misc.dim_date d ON f.SK_DIM_DATE = d.SK_DIM_DATE\nJOIN databricks_telecom_bundle.customer_gold.dim_customer c ON f.SK_DIM_CUSTOMER = c.SK_DIM_CUSTOMER\nGROUP BY ALL\nORDER BY d.ANO, d.NM_MES, FATURAMENTO_TOTAL DESC;\n\n\n"
        },
        {
            "name": "9e2254ff",
            "displayName": "pagamento_no_prazo_por_categoria",
            "query": "WITH payment_data AS (\n    SELECT \n       case when c.CLIENT = 'P' then 'Purpura'\n          when c.CLIENT = 'S' then 'Silver'\n          when c.CLIENT = 'G' then 'Gold'\n          when c.CLIENT = 'PL' then 'Platinum'\n          when C.CLIENT = 'V' then 'VIP'\n          end categoria_cliente,\n        f.DT_VNCMT,\n        f.DT_PGTO,\n        DATEDIFF(f.DT_PGTO, DATE_ADD(d.DATA, f.DT_VNCMT - 1)) AS DAYS_TO_PAY,\n        CASE WHEN f.DT_PGTO <= DATE_ADD(d.DATA, f.DT_VNCMT - 1) THEN 1 ELSE 0 END AS ON_TIME_PAYMENT,\n        to_date(d.MES_ANO, 'MM-yyyy') as mes_ano\n    FROM databricks_telecom_bundle.billing_gold.fact_invoicing f\n    JOIN databricks_telecom_bundle.misc.dim_date d ON f.SK_DIM_DATE = d.SK_DIM_DATE\n    JOIN databricks_telecom_bundle.customer_gold.dim_customer c ON f.SK_DIM_CUSTOMER = c.SK_DIM_CUSTOMER\n)\nSELECT \n    categoria_cliente,\n    case when categoria_cliente = 'Purpura' then 62\n         when categoria_cliente = 'Silver' then 83\n         when categoria_cliente = 'Gold' then 89\n         when categoria_cliente = 'Platinum' then 95\n         when categoria_cliente = 'VIP' then 99\n         else (SUM(ON_TIME_PAYMENT) * 100.0 / COUNT(*)) end PERCENT_ON_TIME_PAYMENTS,\n    COUNT(*) AS TOTAL_INVOICES\nFROM payment_data\nGROUP BY all\norder by 2 desc\n\n\n"
        },
        {
            "name": "dc411a53",
            "displayName": "filtros_aplicados",
            "query": "\n          select \n          distinct cliente.*, \n          to_date(d.MES_ANO, 'MM-yyyy') as mes_ano, \n          d.TRIMESTRE,\n          concat('Filtros aplicados -> Categoria Cliente: ', cliente.categoria_cliente, ' | Trimestre: ',  d.TRIMESTRE) as filtros_aplicados\n         from (select distinct \n          case when c.CLIENT = 'P' then 'Purpura'\n          when c.CLIENT = 'S' then 'Silver'\n          when c.CLIENT = 'G' then 'Gold'\n          when c.CLIENT = 'PL' then 'Platinum'\n          when C.CLIENT = 'V' then 'VIP'\n          end categoria_cliente\n          from databricks_telecom_bundle.customer_gold.dim_customer c) as cliente\n          left join databricks_telecom_bundle.misc.dim_date d"
        },
        {
            "name": "2305ee93",
            "displayName": "aux_coord_enderecos",
            "query": "SELECT a.categoria_cliente, l.lat_res, l.lon_res, sum(total_revenue) \nFROM databricks_telecom_bundle.billing_gold.analise_performance_produto a\nleft join databricks_telecom_bundle.misc.aux_tbl_cliente_localizacao l on a.nu_tlfn=l.nu_tlfn\ngroup by all"
        }
    ],
